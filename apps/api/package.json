{
  "name": "@qr-smart-order/api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "predev": "npm run prisma:generate",
    "dev": "ts-node-dev --respawn --transpile-only src/main.ts",
    "build": "nest build --webpack false --tsc",
    "prestart": "",
    "start": "ts-node-dev --respawn --transpile-only src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"**/*.{ts,tsx}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist coverage",
    "prisma:generate": "XDG_CACHE_HOME=../../.cache prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "prisma:validate": "prisma validate",
    "prisma:seed": "ts-node prisma/seed.ts",
    "test:prisma": "ts-node src/test-prisma-connection.ts",
    "test:integration": "ts-node src/test-supabase-prisma-integration.ts",
    "test:websocket": "ts-node src/test-websocket.ts",
    "test:websocket:load": "ts-node src/test-websocket-load.ts",
    "debug:supabase": "ts-node src/debug-supabase.ts"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.22",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^10.4.22",
    "@prisma/client": "^6.0.0",
    "@qr-smart-order/shared-types": "*",
    "@supabase/supabase-js": "^2.39.0",
    "helmet": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@qr-smart-order/tsconfig": "*",
    "@types/express": "^4.17.17",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prisma": "^6.0.0",
    "ts-jest": "^29.1.0",
    "socket.io-client": "^4.8.3",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.0.0"
  }
}
